<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--xml方式加载Bean-->
    <bean id="car" class="com.blackcat.configuration.entity.Car"></bean>

    <bean id="driverFactoryBean" class="com.blackcat.factorybean.bean.DriverFactoryBean">
        <property name="jdbcUrl" value="jdbc:mysql://localhost:3306/blog"></property>
    </bean>

</beans>